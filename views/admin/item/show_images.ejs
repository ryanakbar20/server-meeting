<div
  class="tab-pane fade show active"
  id="show-item"
  role="tabpanel"
  aria-labelledby="show-item"
>
  <!-- DataTales Example -->
  <div class="card shadow mb-4">
    <div class="card-header py-3">
      <h6 class="m-0 font-weight-bold text-primary">Image Item</h6>
    </div>
    <div class="card-body">
      <div class="table-responsive">
        <div class="container">
          <div class="row">
            <% for (let i = 0; i < item.imageId.length; i++) { %>
            <div class="col-md-4">
              <div class="card shadow mb-4 mt-2">
                <div class="card-body">
                  <img
                    src="https://server-node-staycation.herokuapp.com/<%= item.imageId[i].imageUrl %>"
                    alt="gambar"
                    width="100%"
                  />
                </div>
              </div>
            </div>
            <% } %>
          </div>
        </div>
        <a href="/admin/item">
          <button class="btn btn-warning">Back</button>
        </a>
      </div>
    </div>
  </div>
</div>

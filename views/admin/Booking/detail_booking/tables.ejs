<div class="card-body">
  <div class="table-responsive">
    <table
      class="table table-bordered"
      id="dataTable"
      width="100%"
      cellspacing="0"
    >
      <thead>
        <tr>
          <th>Member Name</th>
          <th>Total</th>
          <th>Bank From</th>
          <th>Title</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>
            <%= booking.memberId.firstName %> <%= booking.memberId.lastName %>
          </td>
          <td>$<%= booking.total %></td>
          <td><%= booking.payments.bankFrom %></td>
          <td><%= booking.itemId.title %></td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
